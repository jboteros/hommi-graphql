{
  "name": "@hommi/graphql",
  "version": "1.0.0",
  "main": "index.ts",
  "types": "index.ts",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "graphql": "^16.8.1",
